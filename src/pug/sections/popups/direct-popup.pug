                span.select__error Some error text
            .col-12.col-md-6
              .direct-form__checkboxes
                label.checkbox.direct-form__checkbox
                  input(type="checkbox").checkbox__input
                  span
                  .cta.checkbox__text Full-time
                label.checkbox.direct-form__checkbox
                  input(type="checkbox").checkbox__input
                  span
                  .cta.checkbox__text Part-time
                label.checkbox.direct-form__checkbox
                  input(type="checkbox").checkbox__input
                  span
                  .cta.checkbox__text Temp
      //END   item

      //BEGIN item
      .direct-form__item.direct-form__item--slide-wrap
        .container
          .row
            .col-12
              .direct-form__item-border
                h4.h4.direct-form__title Salary per hour
                .direct-form__ui-single-slider
                  include ../../elements/ui-single-slider
      //END   item

      //BEGIN item
