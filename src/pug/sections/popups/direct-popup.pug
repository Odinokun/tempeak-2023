//BEGIN direct-wrap
#direct-popup.popup
  .direct.popup-body
    .popup-close-btn
      +svg('close')

    //BEGIN direct-header
    .direct-header
      .container
        .row
          h1.h1.direct-header__title Direct hire
    //END   direct-header

    //BEGIN direct-tabs
    .direct-tabs
      .container
        .row
          .direct-tabs__inn
            .direct-tabs__item.direct-tabs__item--contractor.active(data-tab="contractors")
              .cta Contractors
            .direct-tabs__item.direct-tabs__item--business(data-tab="business")
              .cta Business Owners
    //END   direct-tabs

    //BEGIN direct-form--contractors
    form.direct-form.direct-form--contractors#direct-form--contractors.active
      //BEGIN item
      .direct-form__item
        .container
          .row
            .col-12
              h4.h4.direct-form__title Personal information
            .col-12.col-md-6
              .input.direct-form__input
                input(placeholder='First name')
                span.input__placeholder First name
                span.input__error Some error text
            .col-12.col-md-6
              .input.direct-form__input
                input(placeholder='Last name')
                span.input__placeholder Last name
                span.input__error Some error text
            .col-12.col-md-6
              .input.direct-form__input
                input(placeholder='Phone')
                span.input__placeholder Phone
                span.input__error Some error text
            .col-12.col-md-6
              .input.direct-form__input
                input(placeholder='Email')
                span.input__placeholder Email
                span.input__error Some error text
      //END   item

      //BEGIN item
      .direct-form__item
        .container
          .row
            .col-12
              h4.h4.direct-form__title Address
